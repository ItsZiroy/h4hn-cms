{
  "kind": "singleType",
  "collectionName": "resumes",
  "info": {
    "singularName": "resume",
    "pluralName": "resumes",
    "displayName": "Resume"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "about": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "email": {
      "type": "email",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "age": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "socials": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::icon.icon"
    },
    "picture": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "workplaces": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::experience.experience"
    },
    "institutions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::experience.experience"
    }
  }
}
