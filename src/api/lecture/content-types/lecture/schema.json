{
  "kind": "collectionType",
  "collectionName": "lectures",
  "info": {
    "singularName": "lecture",
    "pluralName": "lectures",
    "displayName": "Lecture"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "topic": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "start": {
      "type": "datetime"
    },
    "end": {
      "type": "datetime"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "lecture",
        "excercise",
        "presentation",
        "exam"
      ]
    },
    "course": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::course.course",
      "inversedBy": "lectures"
    },
    "files": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
